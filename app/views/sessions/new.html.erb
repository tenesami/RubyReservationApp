<h1> log in  </h1>

<%= form_for(User.new, url: '/login') do |f| %>
<p>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
</p>
<p>
    <%= f.label :email %>
    <%= f.text_field :email %>
</p>
    <%= f.label :password %>
    <%= f.password_field :password %>
<p>
    <%= f.submit "log In" %>
</p>
<% end %> 